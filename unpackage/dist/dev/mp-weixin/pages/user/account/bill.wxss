.account-bill .rf-bill-list .rf-list-item {
  background-color: #fff;
  padding: 20rpx 30rpx;
  border: 1rpx solid rgba(0, 0, 0, 0.02);
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.account-bill .rf-bill-list .rf-list-item .mid {
