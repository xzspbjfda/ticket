page {
  background: #fff;
}
page .wrap {
  padding: 30rpx;
}
page .wrap .subwrap {
  background: #fff;
}
page .wrap .subwrap .u-tag {
  color: #999;
  font-size: 28rpx;
  border: none;
  border-radius: 8px;
  padding: 20rpx 30rpx;
  margin-left: 20rpx;
}
page .wrap .subwrap .u-tag.active {
